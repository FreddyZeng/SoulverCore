rTitle":[{"kind":"identifier","text":"EngineCustomization"}],"abstract":[{"type":"text","text":"A key SoulverCore model object used to customize how you want SoulverCore to interpret expressions"}],"url":"\/documentation\/soulvercore\/enginecustomization","kind":"symbol","identifier":"doc:\/\/app.soulver.core\/documentation\/SoulverCore\/EngineCustomization","title":"EngineCustomization"}},"variantOverrides":[{"traits":[{"interfaceLanguage":"occ"}],"patch":[{"path":"\/identifier\/interfaceLanguage","op":"replace","value":"occ"},{"value":null,"op":"add","path":"\/topicSections"},{"op":"add","value":null,"path":"\/seeAlsoSections"}]}]}